<svg xmlns="http://www.w3.org/2000/svg" width="300" height="300" viewBox="0 0 300 300">
  <!-- Robot body -->
  <rect x="100" y="120" width="100" height="120" rx="10" fill="#4ade80" stroke="#22c55e" stroke-width="2"/>

  <!-- Robot head -->
  <circle cx="150" cy="80" r="40" fill="#4ade80" stroke="#22c55e" stroke-width="2"/>

  <!-- Robot eyes -->
  <circle cx="130" cy="70" r="8" fill="white"/>
  <circle cx="170" cy="70" r="8" fill="white"/>
  <circle cx="130" cy="70" r="4" fill="black"/>
  <circle cx="170" cy="70" r="4" fill="black"/>

  <!-- Robot mouth -->
  <rect x="135" y="95" width="30" height="5" rx="2" fill="black"/>

  <!-- Robot arms -->
  <rect x="70" y="140" width="30" height="80" rx="5" fill="#4ade80" stroke="#22c55e" stroke-width="2"/>
  <rect x="200" y="140" width="30" height="80" rx="5" fill="#4ade80" stroke="#22c55e" stroke-width="2"/>

  <!-- Robot legs -->
  <rect x="110" y="240" width="25" height="50" rx="5" fill="#22c55e" stroke="#16a34a" stroke-width="2"/>
  <rect x="165" y="240" width="25" height="50" rx="5" fill="#22c55e" stroke="#16a34a" stroke-width="2"/>

  <!-- Decorative elements -->
  <circle cx="150" cy="150" r="10" fill="#fbbf24" stroke="#f59e0b" stroke-width="1"/>
</svg>